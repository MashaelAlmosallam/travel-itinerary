<%= form_for @place do |f| %>
  <div>
    <%= f.label :name, "What is the name of the place?" %>
    <%= f.text_field :name %>
  </div>
  <div>
    <%= f.label :description, "Describe the place" %> <br>
    <%= f.text_area :description %>
  </div>
  <div>
  <%= f.label "Image" %>
  <%= f.file_field :image %>
</div>
<div>
    <%= f.label :duration, "Styaing hours ?" %>
    <%= f.text_area :duration %> 
</div>


  <%= f.submit %>
<% end %>